      "source": [
        "#odd even filter\n",
        "\n",
        "try:\n",
        "  num=int(input(\"give me a number\"))\n",
        "  if num % 2 == 0:\n",
        "    print(f\"{num} is even\")\n",
        "  else:\n",
        "    print(f\"{num} is odd\")\n",
        "\n",
        "except ValueError :\n",
        "  print(\"invalid input\")"
      ]
    }
  ]
